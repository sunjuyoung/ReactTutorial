olumn":12}},{"generated":{"line":102,"column":19},"source":"index.js","original":{"line":81,"column":0}},{"generated":{"line":102,"column":22},"source":"index.js","original":{"line":81,"column":20}},{"generated":{"line":102,"column":34},"source":"index.js","original":{"line":81,"column":24}},{"name":"store","generated":{"line":103,"column":0},"source":"index.js","original":{"line":82,"column":4}},{"name":"store","generated":{"line":103,"column":2},"source":"index.js","original":{"line":82,"column":4}},{"generated":{"line":103,"column":7},"source":"index.js","original":{"line":82,"column":9}},{"name":"dispatch","generated":{"line":103,"column":8},"source":"index.js","original":{"line":82,"column":10}},{"generated":{"line":103,"column":16},"source":"index.js","original":{"line":82,"column":4}},{"name":"decrease","generated":{"line":103,"column":17},"source":"index.js","original":{"line":82,"column":19}},{"generated":{"line":103,"column":25},"source":"index.js","original":{"line":82,"column":27}},{"generated":{"line":103,"column":27},"source":"index.js","original":{"line":82,"column":4}},{"generated":{"line":104,"column":0},"source":"index.js","original":{"line":83,"column":1}},{"generated":{"line":104,"column":1},"source":"index.js","original":{"line":81,"column":0}}],"sources":{"index.js":"import {createStore} from 'redux';\r\n\r\n\r\n\r\n//UI관리,별도의 라이브러리 사용하지 않기때문에 DOM직접 수정\r\nconst divToggle = document.querySelector('.toggle');\r\nconst counter = document.querySelector('h1');\r\nconst btnIncrease = document.querySelector('#increase');\r\nconst btnDecrease = document.querySelector('#decrease');\r\n\r\n\r\n//상태에 변화를 주는 액션 , 이름 정의\r\nconst TOGGLE_SWITCH='TOGGLE_SWITCH';\r\nconst INCREASE = 'INCREASE';\r\nconst DECREASE = 'DECREASE';\r\n\r\n//액션 이름을 사용하여 액션 객체를 만드는 액션 생성 함수\r\nconst toggleSwitch=()=>({type:TOGGLE_SWITCH});\r\nconst increase = (difference)=>({type:INCREASE,difference});\r\nconst decrease = ()=>({type:DECREASE});\r\n\r\n//초깃값 설정\r\nconst initialState={\r\n    toggle:false,\r\n    counter:0\r\n};\r\n\r\n\r\n//state가 undefined일때는 initialState를 기본값으로 사용\r\nfunction reducer(state = initialState, action){\r\n    console.log(\"reducer\");\r\n    //action.type에 따른 다른 작업을 처리함\r\n    switch(action.type){\r\n        case TOGGLE_SWITCH:\r\n            return{\r\n                ...state,//불변성 유지를 한다\r\n                toggle:!state.toggle\r\n            };\r\n        case INCREASE:\r\n            return {\r\n                ...state,\r\n                counter:state.counter+action.difference\r\n            };\r\n        case DECREASE:\r\n            return{\r\n                ...state,\r\n                counter:state.counter -1\r\n            };\r\n            default:\r\n            return state;\r\n\r\n    }\r\n}\r\n\r\n//스토어 만들기\r\nconst store = createStore(reducer);\r\n\r\nconst render=()=>{\r\n    console.log(\"render\");\r\n    const state = store.getState(); //현재 상태를 불러온다\r\n    //토글처리\r\n    if(state.toggle){\r\n        divToggle.classList.add('active');\r\n    }else{\r\n        divToggle.classList.remove('active');\r\n    }\r\n\r\n    //카운터처리\r\n    counter.innerText=state.counter;\r\n}\r\nrender();\r\nstore.subscribe(render);\r\n\r\ndivToggle.onClick=()=>{\r\n    store.dispatch(toggleSwitch());\r\n};\r\nbtnIncrease.onClick=()=>{\r\n    store.dispatch(increase(1));\r\n\r\n}\r\nbtnDecrease.onClick=()=>{\r\n    store.dispatch(decrease());\r\n}"},"lineCount":null}},"error":null,"hash":"9a69ba84c179a80d2a931b8d2d513cc3","cacheData":{"env":{}}}